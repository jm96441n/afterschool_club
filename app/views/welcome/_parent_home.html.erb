<h3>Programs Your Child is Attending</h3>
  <div class="row">
    <% programs[0..2].each do |program| %>
      <%= render partial: "programs/program_box", locals: { favorites: favorites, program: program } %>
    <% end %>
  </div>

<h3>Programs Your Child has Suggested</h3>
  <div class="row">
    <% programs[0..2].each do |program| %>
      <%= render partial: "programs/program_box", locals: { favorites: favorites, program: program } %>
    <% end %>
  </div>

<h3>Programs Your Child is Interested in Attending</h3>
  <div class="row">
    <% programs[0..2].each do |program| %>
      <%= render partial: "programs/program_box", locals: { favorites: favorites, program: program } %>
    <% end %>
  </div>
